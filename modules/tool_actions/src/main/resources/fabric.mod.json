  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_1799": ["io/github/fabricators_of_create/porting_lib/tool/extensions/ItemStackExtensions"],
      "net/minecraft/class_2248": ["io/github/fabricators_of_create/porting_lib/tool/extensions/BlockExtensions"],
      "net/minecraft/class_2680": ["io/github/fabricators_of_create/porting_lib/tool/extensions/BlockStateExtensions"]
    }
